<h1>Sign in to your User Account</h1>
<p>Find me in app/views/session/user.html.erb</p>

<%= form_for :data, url: sessions_url do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.hidden_field :type, value: 'user'  %>
  <%= f.submit "Login" %>
<% end %>

